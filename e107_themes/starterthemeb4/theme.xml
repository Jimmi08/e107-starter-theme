<?xml version="1.0" encoding="utf-8"?>
<e107Theme name="Bootstrap 4" version="1.1.2" date="2019-07-26" compatibility="2.0">
	<author name ="Jimako" email="jimako@e107sk.com" url="https://www.e107sk.com/" />
	<summary>Bootstrap4 Starter theme</summary>
	<description>Bootstrap4 Starter theme</description>
	<category>generic</category>
	<plugins>
		<plugin name='jmcore' />
		<plugin name='jmelements' />
		<plugin name='jmtheme' />
		<plugin name='social' url='core' />
		<plugin name='featurebox' url='core' />
	</plugins>
	<keywords>
		<word>bootstrap</word>
		<word>starter</word>
	</keywords>
	<libraries>
		<library name="bootstrap" version="4" scope="all"/>
		<library name="fontawesome" version="5" scope="all"/>
	</libraries>
	<stylesheets>
		<css file="style.css" name="Default"/>
	</stylesheets>
	<screenshots>
		<image>preview_frontend.png</image>
	</screenshots>
	<layouts>
		<layout name='presentation' title='Presentation Page' >
			<custompages>presentation</custompages>
		</layout>
		<layout name='home' title='Home Page' >
			<custompages>FRONTPAGE</custompages>
		</layout>
			<layout name='full' title='Full' >
			<custompages>forum/ download/</custompages>
		</layout>
		<layout name='sidebar_left' title='Sidebar (left)' />
		<layout name='sidebar_right' title='Sidebar (right)' default='true' />
		<layout name='singlelogin' title='Login Screen' >
			<custompages>login fpw</custompages>
		</layout>
			<layout name='singlesignup' title='Register Page' >
			<custompages>signup</custompages>
		</layout>
		<layout name='singlecontact' title='Contact page' >
			<custompages>contact</custompages>
		</layout>
		<layout name='bare' title='Starter Bare template' >
			<custompages>guide</custompages>
		</layout>
	</layouts>
	<themePrefs>
		<pref name='map'>Insert your own iframe code for google maps</pref>
		<pref name='cardmenu_look'>1</pref>
		<pref name='inlinecss'></pref>
		<pref name='inlinejs'></pref>
		<pref name='login_iframe'></pref>
	</themePrefs>
</e107Theme>